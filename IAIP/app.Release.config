<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="Environment" xdt:Transform="Replace" xdt:Locator="Match(key)"
         value="Production" />
    <add key="GecoUrl" xdt:Transform="Replace" xdt:Locator="Match(key)"
         value="https://geco.gaepd.org/" />
	<add key="AirReportsUrl" xdt:Transform="Replace" xdt:Locator="Match(key)"
		value="https://air.gaepd.org/" />
  </appSettings>
</configuration>